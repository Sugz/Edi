<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

    xmlns:reskeys="clr-namespace:MRULib.Themes"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    >
    <!-- Pin Control -->
    <SolidColorBrush x:Key="{x:Static reskeys:ResourceKeys.Pin_Foreground}" Color="#C0FFFFFF" />

    <SolidColorBrush x:Key="{x:Static reskeys:ResourceKeys.Pin_ForegroundBorder}" Color="#FFFFFFFF" />

    <sys:Double x:Key="{x:Static reskeys:ResourceKeys.Pin_NoCheckMarkOpacitiy}">0.75</sys:Double>
    
</ResourceDictionary>
